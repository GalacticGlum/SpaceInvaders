
#----------------------------- Global Properties ----------------------------#

/outputDir:bin
/intermediateDir:obj
/platform:Windows
/config:
/profile:Reach
/compress:False

#-------------------------------- References --------------------------------#

/reference:..\..\ContentPipeline\bin\Debug\SpaceInvaders.ContentPipeline.dll

#---------------------------------- Content ---------------------------------#

#begin BarrierLayout.json
/importer:JsonImporter
/processor:JsonProcessor
/build:BarrierLayout.json

#begin BulletTypes.json
/importer:JsonImporter
/processor:JsonProcessor
/build:BulletTypes.json

#begin EnemyTypeLayers.json
/importer:JsonImporter
/processor:JsonProcessor
/build:EnemyTypeLayers.json

#begin EnemyTypes.json
/importer:JsonImporter
/processor:JsonProcessor
/build:EnemyTypes.json

#begin MainAtlas.meta.json
/importer:JsonImporter
/processor:JsonProcessor
/build:MainAtlas.meta.json

#begin MainAtlas.png
/importer:TextureImporter
/processor:TextureProcessor
/processorParam:ColorKeyColor=255,0,255,255
/processorParam:ColorKeyEnabled=True
/processorParam:GenerateMipmaps=False
/processorParam:PremultiplyAlpha=True
/processorParam:ResizeToPowerOfTwo=False
/processorParam:MakeSquare=False
/processorParam:TextureFormat=Color
/build:MainAtlas.png

#begin SpaceInvadersFont.spritefont
/importer:FontDescriptionImporter
/processor:FontDescriptionProcessor
/processorParam:PremultiplyAlpha=True
/processorParam:TextureFormat=Compressed
/build:SpaceInvadersFont.spritefont

#begin UfoScores.json
/importer:JsonImporter
/processor:JsonProcessor
/build:UfoScores.json

#begin SpaceInvadersFontHeader.spritefont
/importer:FontDescriptionImporter
/processor:FontDescriptionProcessor
/processorParam:PremultiplyAlpha=True
/processorParam:TextureFormat=Compressed
/build:SpaceInvadersFontHeader.spritefont

